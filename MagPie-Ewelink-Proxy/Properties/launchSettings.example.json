{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "state/<DEVICE_ID>",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "EWELINK_EMAIL": "<EMAIL>",
        "EWELINK_PASSWORD": "<PASSWORD>",
        "EWELINK_COUNTRY_CODE": "<COUNTRY_CODE>",
        "EWELINK_REGION": "<REGION>",
        "EWELINK_APP_ID": "<APP_ID>",
        "EWELINK_APP_SECRET": "<APP_SECRET>"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5071"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "state/<DEVICE_ID>",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "EWELINK_EMAIL": "<EMAIL>",
        "EWELINK_PASSWORD": "<PASSWORD>",
        "EWELINK_COUNTRY_CODE": "<COUNTRY_CODE>",
        "EWELINK_REGION": "<REGION>",
        "EWELINK_APP_ID": "<APP_ID>",
        "EWELINK_APP_SECRET": "<APP_SECRET>"
      }
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/state/<DEVICE_ID>",
      "environmentVariables": {
        "EWELINK_EMAIL": "<EMAIL>",
        "EWELINK_PASSWORD": "<PASSWORD>",
        "EWELINK_COUNTRY_CODE": "<COUNTRY_CODE>",
        "EWELINK_REGION": "<REGION>",
        "EWELINK_APP_ID": "<APP_ID>",
        "EWELINK_APP_SECRET": "<APP_SECRET>"
      },
      "publishAllPorts": true
    }
  },
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:16097",
      "sslPort": 0
    }
  }
}