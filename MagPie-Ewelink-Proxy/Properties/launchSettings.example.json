{
  "profiles": {
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/state/<SAMPLE_DEVICE_ID>",
      "environmentVariables": {
        "EWELINK_EMAIL": "<EMAIL>",
        "EWELINK_PASSWORD": "<PASSWORD>",
        "EWELINK_COUNTRY_CODE": "<COUNTRY_CODE>",
        "EWELINK_REGION": "<REGION>",
        "EWELINK_APP_ID": "<APP_ID>",
        "EWELINK_APP_SECRET": "<APP_SECRET>"
      },
      "publishAllPorts": true
    }
  },
  "$schema": "https://json.schemastore.org/launchsettings.json"
}